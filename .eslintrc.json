{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/function-component-definition": [2, { "namedComponents": "arrow-function" }],
        "arrow-body-style": 0,
        "quotes": [2, "single", { "avoidEscape": true }],
        "no-unused-vars": 1,
        "react/prop-types": 0
    }
}
